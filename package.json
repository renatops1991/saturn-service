{
  "name": "saturn-service",
  "version": "4.3.0",
  "description": "The Saturn service is an API used for financial organization",
  "scripts": {
    "start": "node -r module-alias/register ./dist/main/server.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --watch src ./src/main/server.ts",
    "start:debug": "nodemon -L --watch ./dist -r module-alias/register --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json -w",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:e2e": "npm test -- --watch -c jest-e2e.config.js",
    "test:ci": "npm test -- --coverage",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace --findRelatedTests",
    "lint": "eslint src --ext ts",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.1.4",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mockdate": "^3.0.5",
    "rimraf": "^5.0.0",
    "standard-version": "^9.5.0",
    "sucrase": "^3.32.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongodb": "^5.5.0",
    "nodemon": "^2.0.22",
    "validator": "^13.9.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
