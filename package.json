{
  "name": "saturn-service",
  "version": "1.0.1",
  "description": "The Saturn service is an API used for financial organization",
  "scripts": {
    "start:dev": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:e2e": "npm test -- --watch -c jest-e2e.config.js",
    "test:ci": "npm test -- --coverage",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace --findRelatedTests",
    "prune": "ts-prune --error",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.6",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.7.2",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "sucrase": "^3.28.0",
    "supertest": "^6.3.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "validator": "^13.7.0"
  },
  "_moduleAliases": {
    "@": "src"
  }
