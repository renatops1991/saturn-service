{
  "name": "saturn-service",
  "version": "2.2.0",
  "description": "The Saturn service is an API used for financial organization",
  "scripts": {
    "start": "node -r module-alias/register ./dist/main/server.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --watch src ./src/main/server.ts",
    "start:debug": "nodemon -L --watch ./dist -r module-alias/register --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "build": "rimraf dist && tsc -w",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:e2e": "npm test -- --watch -c jest-e2e.config.js",
    "test:ci": "npm test -- --coverage",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace --findRelatedTests",
    "lint": "eslint src --ext ts",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.2",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.8",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "sucrase": "^3.28.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "mongodb": "^4.11.0",
    "nodemon": "^2.0.20",
    "validator": "^13.7.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}